#include <stdio.h>
int main()
{
   char s[100], r[100];
   int start , end, count = 0;

   printf("Input a string");
   scanf("%s", s)

   while (s[count] != '\0')
      count++;

   end = count - 1;

   for (start = 0; start< count; start++)
   {
      r[start] = s[end];
      end-- ;
   }

   r[start] = '\0';

   printf("%s\n", r);
}

